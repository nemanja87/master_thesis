{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "OpenIddict": {
    "Issuer": "https://localhost:5001",
    "UseDevelopmentCertificates": true,
    "Clients": {
      "bench-ui": {
        "RedirectUri": "https://127.0.0.1:5173/auth/callback",
        "PostLogoutRedirectUri": "https://127.0.0.1:5173/"
      },
      "bench-runner": {
        "ClientSecret": "bench-runner-secret"
      },
      "service-order": {
        "ClientSecret": "service-order-secret"
      }
    }
  }
}
